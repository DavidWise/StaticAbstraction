<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.0;netstandard2.1;netcoreapp2.2;netstandard2.0;net45;net46;net47;net48;net40</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>David Wise</Authors>
    <Company />
    <Copyright>@2019 David Wise</Copyright>
    <PackageId>StaticAbstraction</PackageId>
    <PackageReleaseNotes></PackageReleaseNotes>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Description>This library creates a way to abstract static calls from common .Net objects so that the containing code can be more easily unit tested</Description>
    <RepositoryUrl>https://github.com/DavidWise/StaticAbstraction.git</RepositoryUrl>
    <Product>Static Abstraction</Product>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Version>0.1.0</Version>
    <PackageProjectUrl>https://github.com/DavidWise/StaticAbstraction</PackageProjectUrl>
    <PackageTags>UnitTests nunit mock static abstraction</PackageTags>
    <PackageLicenseUrl>https://github.com/DavidWise/StaticAbstraction/blob/develop/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <PackageIconUrl>https://photos.smugmug.com/photos/i-dpvBV8k/0/65dc66a6/O/i-dpvBV8k.png</PackageIconUrl>
  </PropertyGroup>
  
	<PropertyGroup Condition=" '$(TargetFramework)' == 'netstandard2.0'">
		<DefineConstants>NETCORE;NETSTANDARD;NETSTANDARD2_0</DefineConstants>
	</PropertyGroup>
  <PropertyGroup Condition=" '$(TargetFramework)' == 'netstandard2.1'">
    <DefineConstants>NETCORE;NETSTANDARD;NETSTANDARD2_1</DefineConstants>
  </PropertyGroup>
	<PropertyGroup Condition=" '$(TargetFramework)' == 'net40'">
		<DefineConstants>NET40;NETFULL</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(TargetFramework)' == 'net45'">
		<DefineConstants>NET45;NETFULL</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(TargetFramework)' == 'net46'">
		<DefineConstants>NET46;NETFULL</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(TargetFramework)' == 'net47'">
		<DefineConstants>NET47;NETFULL</DefineConstants>
	</PropertyGroup>	
	<PropertyGroup Condition=" '$(TargetFramework)' == 'net48'">
		<DefineConstants>NET48;NETFULL;NETSTANDARD;NETSTANDARD2_0</DefineConstants>
	</PropertyGroup>	
	<PropertyGroup Condition=" '$(TargetFramework)' == 'netcoreapp2.2'">
		<DefineConstants>NETCORE22;NETCORE</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(TargetFramework)' == 'netcoreapp3.0'">
		<DefineConstants>NETCORE30;NETCORE</DefineConstants>
	</PropertyGroup>
</Project>
